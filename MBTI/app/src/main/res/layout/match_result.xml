<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- 최종 매칭 결과를 보여주는 화면이다.-->



    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="35dp"
        android:text="나와 가장 잘 맞을 것 같은 룸메이트!"
        android:textColor="#03A9F4"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="54dp" />

    <TextView
        android:id="@+id/first_rank_mbti_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="102dp"
        android:text="MBTI"
        android:textColor="#03A9F4"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/second_rank_mbti_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="272dp"
        android:text="MBTI"
        android:textColor="#03A9F4"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/third_rank_mbti_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="451dp"
        android:text="MBTI"
        android:textColor="#03A9F4"
        android:textSize="24sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/first_rank_mbti_image"
        android:layout_width="76dp"
        android:layout_height="101dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="142dp"
        app:srcCompat="@drawable/mbti_enfj" />

    <ImageView
        android:id="@+id/second_rank_mbti_image"
        android:layout_width="76dp"
        android:layout_height="101dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="313dp"
        app:srcCompat="@drawable/mbti_intj" />

    <ImageView
        android:id="@+id/third_rank_mbti_image"
        android:layout_width="76dp"
        android:layout_height="101dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="493dp"
        app:srcCompat="@drawable/mbti_esfp" />


    <TextView
        android:id="@+id/first_rank_match"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="172dp"
        android:layout_marginEnd="120dp"
        android:text="Match 1"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/second_rank_match"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="101dp"
        android:layout_marginEnd="119dp"
        android:layout_toEndOf="@+id/second_rank_mbti_image"
        android:text="Match 2"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/third_rank_match"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="521dp"
        android:layout_marginEnd="119dp"
        android:layout_toEndOf="@+id/third_rank_mbti_image"
        android:text="Match 3"
        android:textSize="30sp" />


</RelativeLayout>